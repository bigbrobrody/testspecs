  <chapter xml:id="tc.ProfileS.ProfileMandatoryFeatures">
    <title>Test Cases for Profile Mandatory Features</title>
    <xi:include href="parts/features/core/HTTPDigest.xml" xpointer="tc.HTTPDigest"/>
